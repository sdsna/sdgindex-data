{
  "name": "@sdgindex/data",
  "version": "0.1.2",
  "description": "",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:cjs && npm run build:es",
    "build:cjs": "trash cjs/* && cross-env NODE_ENV=cjs babel src --out-dir cjs",
    "build:es": "trash es/* && cross-env NODE_ENV=es babel src --out-dir es",
    "test": "jest",
    "docs": "./node_modules/.bin/esdoc",
    "prepare": "npm run build",
    "release": "np"
  },
  "keywords": [],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "author": "",
  "license": "ISC",
  "files": [
    "parse",
    "data",
    "cjs",
    "es"
  ],
  "jest": {
    "moduleNameMapper": {
      "@root(.*)$": "<rootDir>/$1"
    },
    "resetMocks": true
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "esdoc": "^1.1.0",
    "esdoc-custom-theme": "^1.4.2",
    "esdoc-importpath-plugin": "^1.0.2",
    "esdoc-standard-plugin": "^1.0.0",
    "jest": "^26.6.3",
    "np": "^7.4.0",
    "trash-cli": "^4.0.0"
  },
  "dependencies": {
    "fs-extra": "^9.1.0",
    "xlsx": "^0.16.9"
  }
}
